{
  "name": "center-and-variability",
  "version": "1.0.0-dev.13",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/center-and-variability.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "CENTER_AND_VARIABILITY",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "phetLibs": [
      "bamboo",
      "twixt",
      "vegas"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsInteractiveDescription": true
    }
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/no-explicit-any": "error",
          "@typescript-eslint/explicit-member-accessibility": "off"
        }
      }
    ]
  }
}